rder500(1, true, 500); // 输出：500 元定金预购, 得到 100 优惠券\n    order500(1, false, 500); // 输出：普通购买, 无优惠券\n    order500(2, true, 500); // 输出：200 元定金预购, 得到 500 优惠券\n    order500(3, false, 500); // 输出：普通购买, 无优惠券\n    order500(3, false, 0); // 输出：手机库存不足\n    </script>\n</body>\n\n</html>",
